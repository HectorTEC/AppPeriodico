<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi Periodico ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Mi Periodico</a>


            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">

                    <li>@Html.ActionLink("Publicaciones", "Index", "Home")</li>
                    <li>@Html.ActionLink("Usuarios", "Usuario", "Home")</li>
                    <li>@Html.ActionLink("Categorias", "Categoria", "Home")</li>
                    <li>@Html.ActionLink("Tipos De Usuarios", "TipoUsuario", "Home")</li>
                    <li>@Html.ActionLink("Tipos de Publicacion", "Index", "TipoPublicacion")</li>
                    <li>@Html.ActionLink("Comentarios", "Index", "Comentario")</li>
                    <li>@Html.ActionLink("Cerrar Sesión", "Login", "Acceso")</li>

                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year -   Mi Periodico.NET</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
